{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "install": {
      "cache": true
    },
    "dev": {
      "persistent": true,
      "cache": true
    }
  },
  "style": "default",
  "tailwind": {
    "config": "",
    "css": "",
    "baseColor": "",
    "cssVariables": false
  },
  "rsc": false,
  "aliases": {
    "utils": "",
    "components": ""
  }
}