{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "cache": true
    },
    "install": {
      "cache": true
    },
    "dev": {
      "persistent": true,
      "cache": true
    }
  }
}