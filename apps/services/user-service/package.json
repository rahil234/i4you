    "@protobuf-ts/runtime": "^2.9.5",
    "@protobuf-ts/runtime-rpc": "^2.9.5",
    "@repo/env-config": "workspace:*",
    "@repo/http-logger": "workspace:*",
    "@repo/proto-files": "workspace:*",
    "@trpc/server": "^10.45.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "morgan": "~1.9.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
