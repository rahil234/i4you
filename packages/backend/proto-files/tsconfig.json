{
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "node16",
    "module": "Node16",
    "baseUrl": "./",
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "generated/*": [
        "./generated/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "generated/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
