{
  "name": "@i4you/proto-files",
  "version": "1.0.3",
  "description": "",
  "type": "commonjs",
  "exports": {
    "./client/*": {
      "types": "./dist/src/client/*.d.ts",
      "default": "./dist/src/client/*.js"
    },
    "./server/*": {
      "types": "./dist/src/server/*.d.ts",
      "default": "./dist/src/server/*.js"
    },
    "./generated/*": {
      "types": "./dist/generated/*.d.ts",
      "default": "./dist/generated/*.js"
    }
  },
  "scripts": {
    "prebuild": "buf generate",
    "build": "tsc && tsc-alias"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@grpc/grpc-js": "^1.13.3"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.52.1",
    "ts-proto": "^2.7.0",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "src",
    "buf.gen.yaml",
    "buf.yaml",
    "generated"
  ],
  "author": "Rahil234",
  "license": "ISC"
}
