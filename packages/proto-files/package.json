{
  "name": "proto-files",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "exports": {
    "./client/*": {
      "types": "./dist/src/clients/*.d.ts",
      "default": "./dist/src/clients/*.js"
    },
    "./server/*": {
      "types": "./dist/src/servers/*.d.ts",
      "default": "./dist/src/servers/*.js"
    },
    "./generated/*": {
      "types": "./dist/generated/*",
      "default": "./dist/generated/*"
    }
  },
  "scripts": {
    "prebuild": "buf generate",
    "build": "tsc && tsc-alias"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@grpc/grpc-js": "^1.13.3"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.52.1",
    "ts-proto": "^2.7.0",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.8.3"
  }
}
