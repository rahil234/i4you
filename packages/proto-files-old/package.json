{
  "name": "@repo/proto-files",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist",
    "proto"
  ],
  "exports": {
    "./*": "./dist/*",
    "./generated/*": "./dist/generated/*"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist/generated && cp -r src/generated dist/generated",
    "proto:generate": "sh scripts/build.sh",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.1",
    "@grpc/proto-loader": "^0.7.13",
    "@protobuf-ts/runtime": "^2.9.5",
    "@protobuf-ts/runtime-rpc": "^2.9.5",
    "glob": "^11.0.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "grpc-tools": "^1.13.0",
    "grpc_tools_node_protoc_ts": "^5.3.3"
  }
}
