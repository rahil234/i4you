{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node src/server.ts",
    "dev": "nodemon",
    "build": "tsc --project ./tsconfig.json",
    "proto": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/proto --proto_path=src/proto src/proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.13",
    "@trpc/server": "^10.45.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.21.2",
    "morgan": "~1.9.1",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.9",
    "ts-proto": "^2.6.1",
    "typescript": "^5.8.2"
  }
}
